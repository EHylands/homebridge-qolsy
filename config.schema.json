{
  "pluginAlias": "QolsysPanel",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "Name": {
        "title": "Name",
        "type": "string",
        "required": true,
        "default": "Qolsys Panel"
      },
      "Host":{
        "title":"Host",
        "type":"string",
        "required": true
      },
      "Port":{
        "title":"Port",
        "type": "integer",
        "required": true,
        "default" : 12345
      },
      "SecureToken":{
        "title":"Secure Token",
        "type": "string",
        "required": true,
        "default": ""
      },
      "UserPinCode":{
        "title":"User Pin Code",
        "type": "string",
        "required": true,
        "default": ""
      },
      "LogPartition":{
        "type": "boolean",
        "title": "Show Partition notifications in log file",
        "default": true
      },
      "LogZone":{
        "type": "boolean",
        "title": "Show Zone notifications in log file",
        "default": false
      }
    }
  }  
}



           